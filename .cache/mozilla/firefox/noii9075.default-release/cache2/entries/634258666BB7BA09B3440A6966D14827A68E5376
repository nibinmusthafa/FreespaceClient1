    function loadFont(url) {
        const head = document.getElementsByTagName('head')[0];
        const lnk = document.createElement('link');
        lnk.rel = "stylesheet";
        lnk.href = url;
        head.appendChild(lnk);
    }
    loadFont('https://fonts.googleapis.com/css?family=Oswald&display=swap')
</script>

